<div class="container">

    {{> nav-partial }}

    {{> header-partial }}

    <main>
        <div class="container">
            <h3 class="text-center">Sign in</h3>
            <br>
            <form>
                <div class="form-group">
                    <input type="email" class="form-control" id="email" aria-describedby="emailHelp"
                        placeholder="Email address" value="email@email.com">
                </div>
                <div class="form-group">
                    <input type="password" class="form-control" id="password" placeholder="Password" value="password">
                </div>
                <div class="form-check">
                    <a class="btn btn-info" style="float: right;" href="/funkoshop/views/index"
                        onclick="Controller.controllers.signin.signin_clicked(event);">Sign in</a>
                </div>

            </form>

        </div> <br><br>
    </main>

    {{> footer-partial }}
</div>